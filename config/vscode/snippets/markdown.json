{
	"Create a matrix":{
		"prefix": "\\mathbf",
		"body": [
			"$$\\mathbf ${1:X}=\\begin{bmatrix}",
			"\\vec ${1:${1/(.*)/${1:/downcase}/}}_1^T\\\\\\",
			"\\vec ${1/(.*)/${1:/downcase}/}_2^T\\\\\\",
			"\\vdots\\\\\\",
			"\\vec ${1:/downcase}_${2:n}^T\\end{bmatrix}",
			"=\\begin{bmatrix}${1:\\downcase}_{11}&${1:\\downcase}_{12}&\\cdots&${1:\\downcase}_{1${3:d}}\\\\\\",
			"${1:\\downcase}_{21}&${1:\\downcase}_{22}&\\cdots&${1:\\downcase}_{2$3}\\\\\\",
			"\\vdots&\\vdots&\\ddots&\\vdots\\\\\\",
			"${1:\\downcase}_{${2}1}&${1:\\downcase}_{${2}2}&\\cdots&${1:\\downcase}_{$2$3}\\end{bmatrix}$$"
		],
		"description": "Create a matrix"
	},
